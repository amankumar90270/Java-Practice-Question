
import java.util.*;
class HelloWorld {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        
    System.out.println("Enter a Number for it's Multiplication : ");
        int num1 = sc.nextInt();
        System.out.println("Table of "+ num1 + " is: ");
        for(int i=0; i<10; i++){
            System.out.println(num1 + " X " +(i+1)+ " = " +num1*(i+1));
        }
        
    }
}
